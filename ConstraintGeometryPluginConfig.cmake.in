# CMake package configuration file for the ConstraintGeometryPlugin plugin

@PACKAGE_INIT@

find_package(SofaGeneral REQUIRED)

if(NOT TARGET ConstraintGeometryPlugin)
        include("${CMAKE_CURRENT_LIST_DIR}/ConstraintGeometryPluginTargets.cmake")
endif()

check_required_components(ConstraintGeometryPlugin)

set(ConstraintGeometryPlugin_LIBRARIES "ConstraintGeometryPlugin")
set_and_check(CONSTRAINTGEOMETRYPLUGIN_INCLUDE_DIRS "@CONSTRAINTGEOMETRYPLUGIN_INCLUDE_DIRS@")
